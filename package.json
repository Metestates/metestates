{
  "name": "metestates",
  "description": "The new digital land-rush is upon us! Will you become the next land-baron?",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/client": "^3.5.5",
    "@apollo/config": "file:./apollo.config.js",
    "@craco/craco": "^6.4.2",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "memoize-one": "^6.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "react-window": "^1.8.6",
    "typescript": "^4.1.2",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "postinstall": "npm run apollo:download-schema && npm run apollo:codegen",
    "apollo:download-schema": "apollo client:download-schema ./marketplace.schema.json",
    "apollo:codegen": "apollo codegen:generate --localSchemaFile=./marketplace.schema.json --target=typescript --tagName=gql",
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@emotion/core": "^11.0.0",
    "@emotion/css": "^11.5.0",
    "@types/graphql": "^14.5.0",
    "@types/react-window": "^1.8.5",
    "autoprefixer": "^9.8.8",
    "postcss": "^7.0.39",
    "tailwind": "^4.0.0",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.2.17"
  }
}
